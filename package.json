  },
  "dependencies": {},
  "devDependencies": {
    "chai": "1.x.x",
    "coveralls": "^2.11.1",
    "event-stream": "^3.1.7",
    "istanbul": "^0.3.0",
    "mocha": "1.x.x",
    "through2": "^0.6.1"
